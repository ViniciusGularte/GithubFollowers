{
    "manifest_version": 1,
    "name": "[GithubFollowers]",
    "description": "[Extensão para identificar de forma fácil seus seguidores do github.]",
    "version": "0.0.1",
    "icons": {
     "48": "icons/icon.png"
     },
    "permissions": [
    "activeTab"
     ],
       "browser_action": {
         "default_title":"GithubFollowers",
         "default_icon": {
         "19": "icons/icon.png",
         "38": "icons/icon.png"
       },
       "default_title": "GithubFollowers"
     },
    "homepage_url": "https://github.com/ViniciusGularte/GithubFollowers",
    "content_scripts": [
        {
          "matches": ["*://github.com/*"],
          "js": ["filter.js"]
        }
    ]
}
